{
  "name": "why-did-i-block",
  "version": "1.0.0",
  "description": "A site to track details about why someone was blocked.",
  "main": "dist/index.js",
  "repository": "git@github.com:joequincy/why-did-i-block.git",
  "author": "Jon Peterson <github@joequincy.dev>",
  "license": "MIT",
  "dependencies": {
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.14",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently --raw \"yarn dev.build\" \"yarn dev.start\"",
